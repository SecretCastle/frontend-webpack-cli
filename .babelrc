{
	"presets": ["@babel/preset-env", "@babel/preset-react"],
	"plugins": [
		[
			"@babel/plugin-transform-runtime", // 前身是babel-pollyfill
			{
				"absoluteRuntime": false,
				"corejs": false,
				"helpers": true,
				"regenerator": true,
				"useESModules": false
			}
		],
		"@babel/plugin-syntax-dynamic-import"
	]
}
